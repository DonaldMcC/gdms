<h1>Upcoming Events</h1>
<table id='TabEvents' class='issuetable'>
				<thead>
					<tr>
						<th>Event</th>                     
                    </tr>
                </thead>
                    <tbody>
{{if events:}}
{{for i,row in enumerate(events):}}
<tr>
<td>{{=A(row.event_name,_href=URL('vieweventmap',args=[row.id],extension='html'))}}</td>
</tr>
<tr>
<td>{{=row.startdatetime.strftime("%d %b %Y %H:%M")}}</td>
</tr>
{{pass}}
{{pass}}
 </tbody>
</table>
